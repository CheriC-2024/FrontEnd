{
  "expo": {
    "name": "CheriC",
    "slug": "cheri-c",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#E52C32"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#E52C32"
      },
      "package": "com.binaryswu.CheriC",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-font"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "041c3b0b-52e7-4f2a-976f-b61dda714bb9"
      }
    }
  }
}
